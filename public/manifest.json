{
  "name": "PacktPub Free Learning Notifier",
  "description": "A Chrome extension that notifies you when a new book is available for free on PacktPub.",
  "author": "Vijay Bhaskar",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [],
  "action": {
    "default_popup": "index.html",
    "default_title": "Free book of the day",
    "default_icon": {
      "16": "/images/book16.png",
      "32": "/images/book32.png",
      "48": "/images/book48.png",
      "128": "/images/book128.png"
    }
  },
  "icons": {
    "16": "/images/book16.png",
    "32": "/images/book32.png",
    "48": "/images/book48.png",
    "128": "/images/book128.png"
  }
}
